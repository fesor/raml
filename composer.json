{
    "name": "fesor/raml",
    "description": "PHP library for working with RAML",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fesor\\RAML\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "support\\Fesor\\RAML\\": "tests/support/",
            "spec\\Fesor\\RAML\\": "tests/spec/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "symfony/yaml": "~2.3.0|~3.0",
        "ql/uri-template": "^1.1",
        "symfony/options-resolver": "^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5",
        "phpunit/phpunit": "^5.3"
    }
}
